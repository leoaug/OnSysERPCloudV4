	<parameter name="idEmpresa" class="java.lang.Integer"/>
	<parameter name="idPedido" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
pe.`DATAEMISSAO` AS dtEmissao,
"" AS mesRefer,
pe.`VLTOTPEDIDO` AS vlFatPedido,
pe.`apvPercentual` AS percInvest,
0 AS vlInvestimento,
0 AS vlPrecoMed,
pe.`OBSLOGISTICA` AS obsLogistica,
pe.`OBSPEDIDO` AS obsPedido,
pe.`apvGerenteRegional` AS gerRegional,
pe.`apvGerenteNacional` AS gerNacional,
pe.`apvDiretor1` AS diretor1,
pe.`apvDiretor2` AS diretor2,
"" AS acaoInicio,
"" AS acaoTermino,
0 AS acaoDias,
p.`CODIGO` AS CodCliente,
p.`RAZAOSOCIAL` AS razaoSocial,
p.`UF` AS uf,
p.`CIDADE` AS cidade,
"" AS regiao,
"" AS gerente,
"" AS contCliente,
"" AS cargoContCli,
p.`EMAIL` AS emailCli,
p.`REPRESENTANTE_id`,
r.`APELIDO` AS representante
FROM pedidovendas pe INNER JOIN participante p ON pe.`CLIENTE_id` = p.id
INNER JOIN representante r ON p.`REPRESENTANTE_id` = r.id
where pe.`EMPRESA_id` = $P{idEmpresa}
 and pe.id = $P{idPedido}]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     pedidocompras.`NRPEDIDO` AS pedidocompras_NRPEDIDO,
     pedidocompras.`DATAEMISSAO` AS pedidocompras_DATAEMISSAO,
 empresa.`logoimg`  AS empresa_logo,
     pedidocompras.`DATAENTREGA` AS pedidocompras_DATAENTREGA,
     pedidocompras.`VLTOTPEDIDO` AS pedidocompras_VLTOTLIQPEDIDO,
     pedidocompras.`SITUACAO` AS pedidocompras_SITUACAO,
     catitem.`DESCRICAO` AS catitem_DESCRICAO
FROM
     `pedidocompras` pedidocompras INNER JOIN `participante` participante ON pedidocompras.`FORNECEDOR_id` = participante.`id`
     INNER JOIN `pedidocomprasparcela` pedidocomprasparcela ON pedidocompras.`id` = pedidocomprasparcela.`PEDIDOCOMPRAS_id`
     INNER JOIN `item` item ON pedidocomprasparcela.`ITEM_id` = item.`id`
     INNER JOIN `empresa`       empresa       ON pedidocompras.`empresa_id` = empresa.`id`
     INNER JOIN `catitem` catitem ON item.`CATITEM_id` = catitem.`id`
$P!{cCondicao}
group by catitem.DESCRICAO, pedidocompras.NRPEDIDO
ORDER BY
     catitem.DESCRICAO asc,
     pedidocompras.DATAEMISSAO ASC,
     participante.NOMEFANTASIA ASC,
     pedidocompras.NRPEDIDO ASC]]>

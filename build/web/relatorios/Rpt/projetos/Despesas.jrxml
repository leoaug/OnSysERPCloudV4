	<queryString>
		<![CDATA[SELECT despesasfa.`DATAMOVTO` AS despesasfa_DATAMOVTO,
  despesasfa.`TIPO`  AS despesasfa_TIPO,
  empresa.`logoimg`  AS empresa_logo,
  despesasfa.`VALOR` AS despesasfa_VALOR,
  usuario.`APELIDO`  AS usuario_APELIDO,
  componenteeap.`DESCRICAO` AS componenteeap_DESCRICAO
  FROM `fichaaprop` fichaaprop
  INNER JOIN `despesasfa`    despesasfa    ON fichaaprop.`id`         = despesasfa.`FICHAAPROP_id`
  INNER JOIN `usuario`       usuario       ON fichaaprop.`USUARIO_id` = usuario.`id`
  INNER JOIN `empresa`       empresa       ON fichaaprop.`empresa_id` = empresa.`id`
  INNER JOIN `componenteeap` componenteeap ON despesasfa.`PACOTE_id`  = componenteeap.`id`
 $P!{cCondicao}]]>
	</queryString>

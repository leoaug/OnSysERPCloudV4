	<queryString>
		<![CDATA[SELECT
     participante.`CODIGO` AS participante_CODIGO,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     participante.`EMAIL` AS participante_EMAIL,
     participante.`FONE` AS participante_FONE,
     participante.`TIPO` AS participante_TIPO,
     participante.`FAX` AS participante_FAX,
     CASE participante.`INATIVO`
	WHEN 'S' THEN "Inativos"
     ELSE
	"Ativos"
     END INATIVO,
     catcliente.`DESCRICAO` AS catcliente_DESCRICAO,
empresa.`logoimg`  AS empresa_logo
FROM  `catcliente` catcliente RIGHT OUTER JOIN `participante` participante ON catcliente.`id` = participante.`CATCLIENTE_id`
 INNER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
 INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
$P!{cCondicao}
ORDER BY
     participante_RAZAOSOCIAL ASC]]>
	</queryString>

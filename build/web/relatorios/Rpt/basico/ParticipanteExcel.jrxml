	<queryString>
		<![CDATA[SELECT
     participante.`CODIGO` AS participante_CODIGO,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     participante.`EMAIL` AS participante_EMAIL,
     participante.`FONE` AS participante_FONE,
     participante.`FAX` AS participante_FAX,
     participante.`TIPO` AS participante_TIPO,
     empresa.`logoimg`  AS empresa_logo,
     representante.`NOME` AS representante_NOME,
     CASE participante.`INATIVO`
	WHEN 'S' THEN "Inativos"
     ELSE
	"Ativos"
     END INATIVO,
     catcliente.`DESCRICAO` AS catcliente_DESCRICAO
FROM
     `participante` participante LEFT OUTER JOIN`representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
 LEFT OUTER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
     LEFT OUTER JOIN `catcliente` catcliente ON participante.`CATCLIENTE_id` = catcliente.`id`
$P!{cCondicao}
ORDER BY participante_RAZAOSOCIAL]]>
	</queryString>

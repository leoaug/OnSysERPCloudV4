	<parameter name="cCodEstIni" class="java.lang.String"/>
	<parameter name="cCodEstFin" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     estado.`DESCRICAO` AS estado_DESCRICAO,
     TRIM(municipio.`CODIGO`) AS municipio_CODIGO,
     municipio.`DESCRICAO` AS municipio_DESCRICAO,
     estado.`UF` AS estado_UF
FROM
     `estado` estado INNER JOIN `municipio` municipio ON estado.`id` = municipio.`ESTADO_id`
WHERE
     estado.`UF` >= $P{cCodEstIni}
     and estado.`UF` <= $P{cCodEstFin}
     and TRIM(municipio.`CODIGO`) >= $P{cCodIni}
     and TRIM(municipio.`CODIGO`) <= $P{cCodFin}
ORDER BY
     estado.`UF`, municipio.`DESCRICAO` ASC]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT distinct
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     subcatitem.`DESCRICAO` AS subcatitem_DESCRICAO,
     empresa.logoimg  AS empresa_logo,
     SUM(itempedidovendas.`QUANTIDADE`) AS itempedidovendas_QUANTIDADE,
     SUM(itempedidovendas.`PERCDESC`) AS itempedidovendas_PERCDESC,
     SUM(itempedidovendas.`VLTOTLIQITEM` + itempedidovendas.`IPI` + itempedidovendas.`VLICMSSUBSTITU`) AS itempedidovendas_VLTOTLIQITEM
FROM
     pedidovendas pedidovendas LEFT OUTER JOIN participante participante ON pedidovendas.CLIENTE_id = participante.id
     LEFT OUTER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     LEFT OUTER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     LEFT OUTER JOIN `subcatitem` subcatitem ON item.`SUBCATITEM_id` = subcatitem.id
     LEFT OUTER JOIN empresa       empresa       ON participante.empresa_id = empresa.id
     LEFT OUTER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN representante representante ON pedidovendas.REPRESENTANTE_id = representante.id
     LEFT OUTER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.`id`
     LEFT OUTER JOIN `tabelaPreco` tabelaPreco ON participante.`TABELAPRECO_id` = tabelaPreco.`id`
$P!{cCondicao}
GROUP BY item_CODIGO,
item_DESCRICAO, subcatitem_DESCRICAO
ORDER BY subcatitem_DESCRICAO, itempedidovendas_QUANTIDADE DESC]]>
	</queryString>

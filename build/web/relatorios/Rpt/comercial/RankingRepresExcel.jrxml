		<defaultValueExpression><![CDATA[" "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT representante.`APELIDO` AS Representante,
       representante.`NOME` AS Nome,
       representante.`EMAIL` AS Email,
       regiao.`DESCRICAO` AS Regiao,
       sum(itemdoctosai.`VLTOTITEM`) AS Valor,

(SELECT sum(itemdoctosai.`VLTOTITEM`)
FROM
     `doctosai` doctosai INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
     INNER JOIN `participante` participante ON doctosai.`PARTICIPANTE_id` = participante.`id`
     INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN `REGIAO` regiao ON representante.`REGIAO_id` = regiao.`id`
     $P!{cCondicao}
     order by sum(itemdoctosai.`VLTOTITEM`) ) AS Valor_tot

FROM
     `doctosai` doctosai INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
     INNER JOIN `participante` participante ON doctosai.`PARTICIPANTE_id` = participante.`id`
     INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN `REGIAO` regiao ON representante.`REGIAO_id` = regiao.`id`
     $P!{cCondicao}
     group by representante.`APELIDO`, representante.`NOME`, representante.`EMAIL`, regiao.`DESCRICAO`
     order by sum(itemdoctosai.`VLTOTITEM`) desc]]>
	</queryString>

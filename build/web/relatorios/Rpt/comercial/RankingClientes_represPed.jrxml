	<queryString>
		<![CDATA[SELECT participante.`nomefantasia` AS NomeFantasia,
       participante.`CNPJCPF` AS cnpj,
       enderecoparticipante.`CIDADE` AS Cidade,
       enderecoparticipante.`UF` AS UF,
       empresa.`logoimg`  AS empresa_logo,
       representante.`APELIDO` AS Representante,
       participante.`FAX` AS part_telefone1,
       participante.`FONE` AS part_telefone2,
       participante.`EMAIL` AS part_email,
       contatoparticipante.`NOME` AS cont_nome,
       contatoparticipante.`FONE` AS cont_telefone1,
       contatoparticipante.`CELULAR` AS cont_telefone2,
       contatoparticipante.`EMAIL` AS cont_email,
       sum(pedidovendas.`VLTOTPEDIDO`) AS Valor,
(SELECT sum(pedidovendas.`VLTOTPEDIDO`)
FROM
     `pedidovendas` pedidovendas INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
     INNER JOIN `representante` representante ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT JOIN `contatoparticipante` contatoparticipante ON participante.`CONTATOPARTICIPANTE_id` = contatoparticipante.id
    $P!{cCondicao}) AS Valor_tot
FROM
     `pedidovendas` pedidovendas INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
     INNER JOIN `representante` representante ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT JOIN `contatoparticipante` contatoparticipante ON participante.`CONTATOPARTICIPANTE_id` = contatoparticipante.id
$P!{cCondicao}
group by participante.`nomefantasia`, participante.`CNPJCPF`, enderecoparticipante.`CIDADE`, enderecoparticipante.`UF`, representante.`APELIDO`
ORDER BY Valor DESC]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
p.nrpedido AS NumPedido,
p.dataemissao AS DtEmissao,
p.dataentrega AS DtValidade,
e.emi_nomeabrev AS Cliente,
e.emi_endereco AS Endereco,
e.emi_bairro AS Bairro,
e.emi_cep AS Cep,
e.emi_fone1 AS Telefone,
e.emi_cidade AS Cidade,
'RJ' AS Estado,
r.rep_nomeabrev AS Vendedor,
it.itm_id AS Codigo,
it.itm_descricao AS Descricao,
i.quantidade AS Quantidade,
i.vlunit AS VlUnit,
(i.quantidade * i.vlunit) AS VlTotal,
0 AS Servico,
0 AS Desconto,
(i.quantidade * i.vlunit) AS VlProposta,
p.obspedido AS Observacao,
'30 Dias' AS PrazoEntrega,
'50% SINAL / 50% ENTREGA' AS CondPag
FROM PEDIDOVENDAS p INNER JOIN ITEMPEDIDOVENDAS i ON p.id = i.pedidovendas_id
INNER JOIN EMITENTE_DW e ON p.EMITENTEDW_emi_id = e.emi_id
INNER JOIN ITEM_DW it ON i.ITEMDW_itm_id = it.itm_id
INNER JOIN REPRES_DW r ON p.REPRESDW_rep_id = r.rep_id
WHERE p.id = 2720]]>
	</queryString>

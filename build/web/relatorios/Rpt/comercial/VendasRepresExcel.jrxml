	<queryString>
		<![CDATA[SELECT
     regiao.`CODIGO` AS regiao_CODIGO,
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     representante.`NOME` AS representante_NOME,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`APELIDO` AS representante_APELIDO,
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
     pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     serienotafiscal.`CODIGO` AS serienotafiscal_CODIGO,
     doctosai.`NRODOCTO` AS doctosai_NRODOCTO,
     doctosai.`DTEMISSAO` AS doctosai_DTEMISSAO,
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     unidademedida.`DESCRICAO` AS unidademedida_DESCRICAO,
     itemdoctosai.`QUANTIDADE` AS itempedidovendas_QUANTIDADE,
     itemdoctosai.`VLTOTITEM` AS itempedidovendas_VLTOTITEM,
     itemdoctosai.`VLTOTLIQITEM` AS itempedidovendas_VLTOTLIQITEM,
     itemdoctosai.`VLUNIT` AS itempedidovendas_VLUNIT,
     itemdoctosai.`VLIPI` AS itemdoctosai_VLIPI,
     itemdoctosai.`VLICMSSUBSTITU` AS itemdoctosai_VLICMSSUBSTITU,
     itemdoctosai.`DESCONTO` AS itemdoctosai_DESCONTO,
     itemdoctosai.`VLICMS` AS itemdoctosai_VLICMS,
     unidademedida.`UN` AS unidademedida_UN
FROM
`doctosai` doctosai INNER JOIN `participante` participante on participante.`id` = doctosai.`PARTICIPANTE_id`
     INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
     INNER JOIN `representante` representante ON representante.`id` = participante.`REPRESENTANTE_id`
     INNER JOIN `serienotafiscal` serienotafiscal ON doctosai.`SERIE_id` = serienotafiscal.`id`
     INNER JOIN `unidademedida` unidademedida ON item.`UNIDADEMEDIDA_id` = unidademedida.`id`
INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN `regiao` regiao ON regiao.`id` = representante.`REGIAO_id`
     INNER JOIN `pedidovendas` pedidovendas ON doctosai.`NRPEDCLI` = pedidovendas.`NRPEDCLI` AND
                      doctosai.`EMPRESA_id` = pedidovendas.`EMPRESA_id`
$P!{cCondicao}]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     regiao.`CODIGO` AS regiao_CODIGO,
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`APELIDO` AS representante_APELIDO,
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     unidademedida.`UN` AS unidademedida_UN,
     unidademedida.`DESCRICAO` AS unidademedida_DESCRICAO,
     itempedidovendas.`QUANTIDADE` AS itempedidovendas_QUANTIDADE,
     itempedidovendas.`VLTOTITEM` AS itempedidovendas_VLTOTITEM,
     itempedidovendas.`IPI` AS itempedidovendas_IPI,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO
FROM
     `pedidovendas` pedidovendas INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     INNER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     INNER JOIN `unidademedida` unidademedida ON item.`UNIDADEMEDIDA_id` = unidademedida.`id`
     INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.`id`]]>
	</queryString>

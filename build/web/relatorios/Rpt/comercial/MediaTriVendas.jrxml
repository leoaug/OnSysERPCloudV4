	<queryString>
		<![CDATA[SELECT catitem.CODIGO AS catitem_CODIGO,
     subcatitem.CODIGO AS subcatitem_CODIGO,
     item.CODIGO AS item_CODIGO,
     item.DESCRICAO AS item_DESCRICAO,
     unidademedida.UN AS unidademedida_UN,
     catitem.DESCRICAO AS catitem_DESCRICAO,
     subcatitem.DESCRICAO AS subcatitem_DESCRICAO,
    (select sum(movtoestoque.QTDMOVTO) / 3
                FROM  movtoestoque movtoestoque
                where  movtoestoque.TIPOMOVTO = 'S'
                and item.id = movtoestoque.item_id
  $P!{cCondicao}) as MEDT,
    (select sum(movtoestoque.QTDMOVTO) / 6
                FROM  movtoestoque movtoestoque
                where  movtoestoque.TIPOMOVTO = 'S'
                and item.id = movtoestoque.item_id
  $P!{cCondicaoS}) as MEDS,
    (select sum(movtoestoque.QTDMOVTO) / 12
                FROM  movtoestoque movtoestoque
                where  movtoestoque.TIPOMOVTO = 'S'
                and item.id = movtoestoque.item_id
  $P!{cCondicaoA}) as MEDA
FROM catitem catitem INNER JOIN item item ON catitem.id = item.CATITEM_id
     INNER JOIN subcatitem subcatitem ON item.SUBCATITEM_id = subcatitem.id
     INNER JOIN unidademedida unidademedida ON item.UNIDADEMEDIDA_id = unidademedida.id
     INNER JOIN movtoestoque movtoestoque ON item.id = movtoestoque.item_id
where item.empresa_id = $P{idEmpresa}
  and movtoestoque.TIPOMOVTO = 'S'
 AND ITEM.CODIGO >= $P!{cCodIni}
 AND ITEM.CODIGO <= $P!{cCodFin}
group by item.id ORDER BY catitem.CODIGO, item.DESCRICAO]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT distinct
     (SELECT SUM(1)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON pedidovendasX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id
 and pedidovendasX.VLTOTPEDIDO > 0
  AND pedidovendasX.SITUACAO <> 'Cancelado'
  AND pedidovendasX.SITUACAO <> 'Rejeitado'
  AND pedidovendasX.SITUACAO <> 'Pendente'
  AND pedidovendasX.SITUACAO <> 'Bloqueado'
  and pedidovendasX.SITUACAO <> 'Obs') AS Real_PEDIDO,
    (SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON pedidovendasX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id
and pedidovendasX.VLTOTPEDIDO > 0
  AND pedidovendasX.SITUACAO <> 'Cancelado'
  AND pedidovendasX.SITUACAO <> 'Rejeitado'
  AND pedidovendasX.SITUACAO <> 'Pendente'
  AND pedidovendasX.SITUACAO <> 'Bloqueado'
  and pedidovendasX.SITUACAO <> 'Obs') AS Real_VALOR,
    CASE WHEN
       (SELECT MAX(metapremiofaixa.`PREMIO`)
        FROM metapremiofaixa
        WHERE (metapremiofaixa.METAPREMIO_ID = metapremio.id) and (metapremiofaixa.`PERCINI` <= (((SELECT SUM(1)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id
and pedidovendasX.VLTOTPEDIDO > 0
  AND pedidovendasX.SITUACAO <> 'Cancelado'
  AND pedidovendasX.SITUACAO <> 'Rejeitado'
  AND pedidovendasX.SITUACAO <> 'Pendente'
  AND pedidovendasX.SITUACAO <> 'Bloqueado'
  and pedidovendasX.SITUACAO <> 'Obs'))/metapremiorepres.`METAPEDIDO`)*100))) IS NULL THEN 0
    ELSE
       (SELECT MAX(metapremiofaixa.`PREMIO`)
        FROM metapremiofaixa
        WHERE (metapremiofaixa.METAPREMIO_ID = metapremio.id) and (metapremiofaixa.`PERCINI` <= (((SELECT SUM(1)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id))/metapremiorepres.`METAPEDIDO`)*100)))
    END AS Premio,
   CASE WHEN
       (SELECT MAX(metapremiofaixa.`PREMIO`)
        FROM metapremiofaixa
        WHERE (metapremiofaixa.METAPREMIO_ID = metapremio.id) and (metapremiofaixa.`PERCINI` <= (((SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id))/metapremiorepres.`METAVALOR`)*100))) IS NULL THEN 0
    ELSE
      ((SELECT MAX(metapremiofaixa.`PREMIO`)
        FROM metapremiofaixa
        WHERE (metapremiofaixa.METAPREMIO_ID = metapremio.id) and (metapremiofaixa.`PERCINI` <= (((SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id))/metapremiorepres.`METAVALOR`)*100)))*2)
     END AS Comissao,
    (((SELECT SUM(1)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id and pedidovendasX.VLTOTPEDIDO > 0
  AND pedidovendasX.SITUACAO <> 'Cancelado'
  AND pedidovendasX.SITUACAO <> 'Rejeitado'
  AND pedidovendasX.SITUACAO <> 'Pendente'
  AND pedidovendasX.SITUACAO <> 'Bloqueado'
  and pedidovendasX.SITUACAO <> 'Obs'))/metapremiorepres.`METAPEDIDO`)*100) AS PercPedido,
    (((SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
       INNER JOIN `representante` representanteX ON participanteX.`REPRESENTANTE_id` = representanteX.`id`
       LEFT OUTER JOIN `regiao` regiaoX ON representanteX.`REGIAO_id` = regiaoX.id
       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa} AND (pedidovendasX.DATAEMISSAO >= $P{pDataIni} AND pedidovendasX.DATAEMISSAO <= $P{pDataFim}) AND representanteX.id = representante.id
 and pedidovendasX.VLTOTPEDIDO > 0
  AND pedidovendasX.SITUACAO <> 'Cancelado'
  AND pedidovendasX.SITUACAO <> 'Rejeitado'
  AND pedidovendasX.SITUACAO <> 'Pendente'
  AND pedidovendasX.SITUACAO <> 'Bloqueado'
  and pedidovendasX.SITUACAO <> 'Obs'))/metapremiorepres.`METAVALOR`)*100) AS PercValor,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`NOME` AS representante_NOME,
     metapremiorepres.`METAPEDIDO` AS representante_METAPEDIDO,
     metapremiorepres.`METAVALOR` AS representante_METAVALOR,
     empresa.`logoimg`  AS empresa_logo,
     regiao.`CODIGO` AS regiao_CODIGO,
     regiao.`DESCRICAO` AS regiao_DESCRICAO
FROM
     `representante` representante INNER JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`REPRESENTANTE_id` = representante.`id`
                               and metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
     INNER JOIN `metapremio` metapremio ON metapremiorepres.`METAPREMIO_id` = metapremio.id
     INNER JOIN `empresa` empresa ON representante.empresa_id = empresa.id
     LEFT OUTER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.id
$P!{cCondicao}
ORDER BY regiao_DESCRICAO, representante_NOME]]>
	</queryString>

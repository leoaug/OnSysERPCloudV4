	<parameter name="cPar" class="java.lang.String"/>
	<parameter name="cCondicao" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
CASE WHEN (select COUNT(distinct rx.codigo)
           from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (rx.usuariorep_id = ur.usuario_id)) > 1 THEN (select distinct rx.codigo
from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (substring(rx.codigo, 1, 4) <> '9999') and (rx.usuariorep_id = ur.usuario_id)) ELSE
(select distinct rx.codigo
from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (rx.usuariorep_id = ur.usuario_id)) END AS codSup,
CASE WHEN (select COUNT(distinct rx.`NOME`)
           from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (rx.usuariorep_id = ur.usuario_id)) > 1 THEN (select distinct rx.`NOME`
from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (substring(rx.codigo, 1, 4) <> '9999') and (rx.usuariorep_id = ur.usuario_id)) ELSE
(select distinct rx.`NOME`
from representante rx inner join usurepres ux ON rx.usuariorep_id = ux.usuario_id
where (rx.usuariorep_id = ur.usuario_id)) END AS Supervidor,
r.`CODIGO` as CodRep,
r.`NOME` as Repres,
sc.`CODIGO` as codCategoria,
sc.`DESCRICAO` as Categoria,
e.`logoimg`  AS logo,
0 as meta,
0 as percentual,
SUM(it.`VLTOT`) as Realizado
FROM usurepres ur inner join representante r ON ur.representante_id = r.id
INNER JOIN pedidovendas pe ON pe.`REPRESENTANTE_ID` = r.id
INNER JOIN itempedidovendas it on it.`PEDIDOVENDAS_id` = pe.id
inner join item i ON it.`ITEM_id` = i.id
inner join subcatitem sc ON i.`SUBCATITEM_id` = sc.id
INNER JOIN `empresa` e ON r.empresa_id = e.id
$P!{cCondicao} 
GROUP BY codSup, Supervidor, CodRep, Repres, codCategoria, Categoria, meta, percentual
ORDER BY Supervidor, Repres, Categoria]]>
	</queryString>

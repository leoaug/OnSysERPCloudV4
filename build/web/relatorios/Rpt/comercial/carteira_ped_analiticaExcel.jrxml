	<queryString>
		<![CDATA[SELECT
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
     pedidovendas.`DATAEMISSAO` AS pedidovendas_DATAEMISSAO,
     pedidovendas.`DATAENTREGA` AS pedidovendas_DATAENTREGA,
     pedidovendas.`VLTOTPEDIDO` AS pedidovendas_VLTOTPEDIDO,
     pedidovendas.`VLTOTLIQPEDIDO` AS pedidovendas_VLTOTLIQPEDIDO,
     item.`CODIGO` AS item_CODIGO,
     itempedidovendas.`QUANTIDADE` AS itempedidovendas_QUANTIDADE,
     itempedidovendas.`VLTOTLIQITEM` AS itempedidovendas_VLTOTLIQITEM,
     pedidovendas.`SITUACAO` AS pedidovendas_SITUACAO,
     item.`DESCRICAO` AS item_DESCRICAO
FROM
     `pedidovendas` pedidovendas INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     INNER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
 $P!{cCondicao}]]>
	</queryString>

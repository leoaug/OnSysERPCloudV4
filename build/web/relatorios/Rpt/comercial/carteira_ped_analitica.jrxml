	<queryString>
		<![CDATA[SELECT
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
     pedidovendas.`DATAEMISSAO` AS pedidovendas_DATAEMISSAO,
     pedidovendas.`DATAENTREGA` AS pedidovendas_DATAENTREGA,
     condpagamento.`DESCRICAO`  AS pedidovendas_CONDPAGAMENTO,
     tipodepedido.`DESCRICAO`   AS pedidovendas_TIPODEPEDIDO,
     pedidovendas.`VLTOTPEDIDO` AS pedidovendas_VLTOTPEDIDO,
     pedidovendas.`VLTOTPEDIDO` AS pedidovendas_VLTOTLIQPEDIDO,
     pedidovendas.`OBSLOGISTICA` AS pedidovendas_OBSLOGISTICA,
     pedidovendas.`OBSCREDITO` AS pedidovendas_OBSCREDITO,
     enderecoparticipante.`UF` AS enderecoparticipante_UF,
     empresa.`logoimg`  AS empresa_logo,
     item.`CODIGO` AS item_CODIGO,
     itempedidovendas.`QTDSALDO` AS itempedidovendas_QUANTIDADE,
     itempedidovendas.`PERCDESC` AS itempedidovendas_PERCDESC,
     itempedidovendas.`VLTOTLIQITEM` + itempedidovendas.`IPI` + itempedidovendas.`VLICMSSUBSTITU` AS itempedidovendas_VLTOTLIQITEM,
     itempedidovendas.`VLLIQABERTO` AS itempedidovendas_VLTOTABERTO,
     pedidovendas.`SITUACAO` AS pedidovendas_SITUACAO,
     item.`DESCRICAO` AS item_DESCRICAO,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`APELIDO` AS representante_APELIDO,
     representante.`NOME` AS representante_NOME,
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     1 as nPed,
     (SELECT distinct usuario.`APELIDO`
       FROM
      `representante` representanteS LEFT OUTER JOIN `usurepres` usurepres ON representanteS.`id` = usurepres.representante_id
       LEFT OUTER JOIN `usuario` usuario ON usurepres.usuario_id = usuario.id
       where usuario.`TIPO` = 'Supervisor' and representanteS.id = representante.id and (representanteS.inativo = '' or representanteS.inativo is null or representanteS.inativo = 'N') limit 0,1) AS Supervisor
FROM pedidovendas pedidovendas LEFT OUTER JOIN participante participante ON pedidovendas.CLIENTE_id = participante.id
     LEFT OUTER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     LEFT OUTER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     LEFT OUTER JOIN `subcatitem` subcatitem ON item.`SUBCATITEM_id` = subcatitem.id
     LEFT OUTER JOIN empresa       empresa       ON participante.empresa_id = empresa.id
     LEFT OUTER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN representante representante ON pedidovendas.REPRESENTANTE_id = representante.id
     LEFT OUTER JOIN `condpagamento` condpagamento ON pedidovendas.`CONDPAGAMENTO_id` = condpagamento.`id`
     LEFT OUTER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.`id`
     LEFT OUTER JOIN `tabelaPreco` tabelaPreco ON participante.`TABELAPRECO_id` = tabelaPreco.`id`
     LEFT OUTER JOIN `tipodepedido` tipodepedido ON pedidovendas.`TIPODEPEDIDO_id` = tipodepedido.`id`
$P!{cCondicao}]]>
	</queryString>

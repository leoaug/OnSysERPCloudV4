	<parameter name="cCondicao" class="java.lang.String"/>
	<parameter name="cPar" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT T.PARTICIPANTE_ID,
    participante.`CODIGO` AS participante_CODIGO,
    participante.`NOMEFANTASIA` AS participante_RAZAOSOCIAL,
    participante.`FONE` AS participante_FONE,
    participante.`EMAIL` AS participante_EMAIL,
    enderecoparticipante.`LOGRADOURO` AS enderecoparticipante_LOGRADOURO,
    enderecoparticipante.`NUMERO` AS enderecoparticipante_NUMERO,
    enderecoparticipante.`BAIRRO` AS enderecoparticipante_BAIRRO,
    enderecoparticipante.`CIDADE` AS enderecoparticipante_CIDADE,
enderecoparticipante.`UF` AS enderecoparticipante_UF,
    representante.`NOME` AS representante_NOME,
    regiao.`DESCRICAO` AS regiao_DESCRICAO,
    empresa.`logoimg`  AS empresa_logo,
    (SELECT MAX(D.`DTEMISSAO`) FROM doctosai D
        WHERE (D.`EMPRESA_id` = T.EMPRESA_ID) AND (D.`PARTICIPANTE_id` = T.PARTICIPANTE_ID)) AS DTUltimaCompra,
    DATEDIFF(CURDATE(), (SELECT MAX(D.`DTEMISSAO`) FROM doctosai D
        WHERE (D.`EMPRESA_id` = T.EMPRESA_ID) AND (D.`PARTICIPANTE_id` = T.PARTICIPANTE_ID))) as nDias
FROM
    doctosai T INNER JOIN `participante` participante ON T.`PARTICIPANTE_id` = participante.id
    INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
    INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
    INNER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
    LEFT OUTER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.`id`
$P!{cCondicao}
ORDER BY representante_NOME, participante_RAZAOSOCIAL]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     subcatitem.`id` AS subcatitem_id,
     subcatitem.`CODIGO` AS subcatitem_CODIGO,
     subcatitem.`DESCRICAO` AS subcatitem_DESCRICAO,
     subcatitem.`EMPRESA_id` AS subcatitem_EMPRESA_id,
     subcatitem.`ESTABELECIMENTO_id` AS subcatitem_ESTABELECIMENTO_id
FROM
     `subcatitem` subcatitem
WHERE
     subcatitem.`EMPRESA_id` = $P{idEmpresa}
     and subcatitem.`CODIGO` >= $P{cCodIni}
     and subcatitem.`CODIGO` <= $P{cCodFin}]]>

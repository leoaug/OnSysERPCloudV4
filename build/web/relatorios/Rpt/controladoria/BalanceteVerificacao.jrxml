	<queryString>
		<![CDATA[SELECT saldocontabil.`VLCREDITO` AS saldocontabil_VLCREDITO,
       saldocontabil.`VLDEBITO` AS saldocontabil_VLDEBITO,
       saldocontabil.`VLDEBITO` - saldocontabil.`VLCREDITO` AS Saldo,
       planocontas.`id` AS planocontas_id,
       planocontas.`CONTA` AS planocontas_CONTA,
       planocontas.`DESCRICAO` AS planocontas_DESCRICAO,
       saldocontabil.`PERIODO` AS saldocontabil_PERIODO,
       empresa.`logoimg`  AS empresa_logo
FROM `empresa` empresa,
     `saldocontabil` saldocontabil,
     `planocontas` planocontas
where saldocontabil.`EMPRESA_id` = empresa.`id`
  and saldocontabil.`EMPRESA_id` = $P{idEmpresa}
  and saldocontabil.`CONTA_id`   = planocontas.`id`
  and planocontas.`TIPO` = 'Analitica'
  and (saldocontabil.`VLDEBITO` <> 0
    or saldocontabil.`VLCREDITO` <> 0)
$P!{cCondicao}
order by planocontas.`CONTA`]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     item.`DESCRICAO` AS DescItem,
     item.`CODIGO` AS CodItem,
     saldoestoque.`QUANTIDADE` AS Quantidade,
     deposito.`CODIGO` AS CodDep,
     deposito.`DESCRICAO` AS DescDep,
     localizacao.`CODIGO` AS CodLocal,
     localizacao.`DESCRICAO` AS DescLocal
FROM
     `item` item INNER JOIN `saldoestoque` saldoestoque ON item.`id` = saldoestoque.`ITEM_id`
     INNER JOIN `deposito` deposito ON saldoestoque.`DEPOSITO_id` = deposito.`id`
     INNER JOIN `localizacao` localizacao ON saldoestoque.`LOCALIZACAO_id` = localizacao.`id`
     INNER JOIN `empresa` empresa ON saldoestoque.`EMPRESA_id` = empresa.`id`
WHERE
     saldoestoque.`EMPRESA_id` = $P{idEmpresa}
     and item.`CODIGO` >= $P{cCodIni}
     and item.`CODIGO` <= $P{cCodFin}
ORDER BY DescItem]]>
	</queryString>

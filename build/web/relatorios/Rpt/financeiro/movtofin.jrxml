	<queryString>
		<![CDATA[SELECT
     movtofin.`DTMOVTO` AS movtofin_DTMOVTO,
     movtofin.`NRODOCTO` AS movtofin_NRODOCTO,
     tipodoctofinanceiro.`CODIGO` AS tipodoctofinanceiro_CODIGO,
     movtofin.`TPMOVTO` AS movtofin_TPMOVTO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     portador.`DESCRICAO` AS portador_DESCRICAO,
     movtofin.`SERIE` AS movtofin_SERIE,
     tipodoctofinanceiro.`TPDBCR` AS tipodoctofinanceiro_TPDBCR,
case tipodoctofinanceiro.`TPDBCR`
WHEN 'Debito' THEN movtofin.`VLMOVTO` * -1
WHEN 'Credito' THEN movtofin.`VLMOVTO`
END as movtofin_VLMOVTO
FROM
     `participante` participante RIGHT OUTER JOIN `movtofin` movtofin ON participante.`id` = movtofin.`PARTICIPANTE_id`
     INNER JOIN `tipodoctofinanceiro` tipodoctofinanceiro ON movtofin.`TIPODOCTO_id` = tipodoctofinanceiro.`id`
     INNER JOIN `portador` portador ON movtofin.`PORTADOR_id` = portador.`id`
     INNER JOIN `empresa` empresa ON movtofin.`EMPRESA_id` = empresa.`id`
$P!{cCondicao}]]>
	</queryString>

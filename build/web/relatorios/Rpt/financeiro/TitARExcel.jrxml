	<queryString>
		<![CDATA[SELECT
case tipodoctofinanceiro.`TPDBCR`
WHEN 'Debito' THEN  tituloarparcela.`VLSALDO` * -1
WHEN 'Credito' THEN tituloarparcela.`VLSALDO`
END as tituloarparcela_VLSALDO,
     tituloar.`DATAEMISSAO` AS tituloar_DATAEMISSAO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     tituloar.`NRODOCTO` AS tituloar_NRODOCTO,
     tituloar.`SERIE` AS tituloar_SERIE,
     tipodoctofinanceiro.`CODIGO` AS tipodoctofinanceiro_CODIGO,
     tipodoctofinanceiro.`TPDBCR` AS tipodoctofinanceiro_TPDBCR,
case tipodoctofinanceiro.`TPDBCR`
WHEN 'Debito' THEN  tituloarparcela.`VALORPARCELA`  * -1
WHEN 'Credito' THEN tituloarparcela.`VALORPARCELA`
END as tituloarparcela_VALORPARCELA,
     tituloarparcela.`PARCELA` AS tituloarparcela_PARCELA,
     tituloarparcela.`DATAVENCIMENTO` AS tituloarparcela_DATAVENCIMENTO
FROM
     `tituloar` tituloar INNER JOIN `tituloarparcela` tituloarparcela ON tituloar.`id` = tituloarparcela.`TITULOAR_id`
     LEFT OUTER JOIN `participante` participante ON tituloar.`PARTICIPANTE_id` = participante.`id`
     INNER JOIN `tipodoctofinanceiro` tipodoctofinanceiro ON tituloar.`TIPODOCTO_id` = tipodoctofinanceiro.`id`
     INNER JOIN `empresa` empresa ON tituloar.`EMPRESA_id` = empresa.`id`
 $P!{cCondicao}]]>
	</queryString>

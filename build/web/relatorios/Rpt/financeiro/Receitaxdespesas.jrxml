     (SELECT  SUM(onsys.movtofin.`VLMOVTO`)
            FROM onsys.movtofin
            where YEAR(onsys.movtofin.`DTMOVTO`) = $P!{cAno} AND month(onsys.movtofin.`DTMOVTO`) = 1 and onsys.movtofin.`TPMOVTO` = 1 AND onsys.movtofin.`EMPRESA_id` = $P!{cEmpresa}) AS Receita,
     (SELECT  SUM(onsys.movtofin.`VLMOVTO`)
            FROM onsys.movtofin
            where YEAR(onsys.movtofin.`DTMOVTO`) = $P!{cAno} AND month(onsys.movtofin.`DTMOVTO`) = 1 and onsys.movtofin.`TPMOVTO` = 2 AND onsys.movtofin.`EMPRESA_id` = $P!{cEmpresa}) AS Despesas

	<queryString>
		<![CDATA[SELECT
     regiao.`CODIGO` AS regiao_CODIGO,
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     representante.`NOME` AS representante_NOME,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`APELIDO` AS representante_APELIDO,
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
     pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     serienotafiscal.`CODIGO` AS serienotafiscal_CODIGO,
     doctosai.`NRODOCTO` AS doctosai_NRODOCTO,
     doctosai.`DTEMISSAO` AS doctosai_DTEMISSAO,
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     unidademedida.`DESCRICAO` AS unidademedida_DESCRICAO,
     itempedidovendas.`QUANTIDADE` AS itempedidovendas_QUANTIDADE,
     itempedidovendas.`VLTOTITEM` AS itempedidovendas_VLTOTITEM,
     itempedidovendas.`VLTOTLIQITEM` AS itempedidovendas_VLTOTLIQITEM,
     itempedidovendas.`VLUNIT` AS itempedidovendas_VLUNIT,
     unidademedida.`UN` AS unidademedida_UN
FROM
     `regiao` regiao INNER JOIN `representante` representante ON regiao.`id` = representante.`REGIAO_id`
     INNER JOIN `participante` participante ON representante.`id` = participante.`REPRESENTANTE_id`
     INNER JOIN `pedidovendas` pedidovendas ON participante.`id` = pedidovendas.`CLIENTE_id`
     INNER JOIN `doctosai` doctosai ON pedidovendas.`NRPEDIDO` = doctosai.`NRPEDIDO`
     INNER JOIN `serienotafiscal` serienotafiscal ON doctosai.`SERIE_id` = serienotafiscal.`id`
     INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     INNER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     INNER JOIN `unidademedida` unidademedida ON item.`UNIDADEMEDIDA_id` = unidademedida.`id`
 $P!{cCondicao}]]>
	</queryString>

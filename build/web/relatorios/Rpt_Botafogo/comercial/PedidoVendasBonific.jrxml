	<queryString>
		<![CDATA[SELECT
     pedidovendas.`DATAEMISSAO` AS pedidovendas_DATAEMISSAO,
     pedidovendas.`DATAENTREGA` AS pedidovendas_DATAENTREGA,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
     pedidovendas.`NRPEDREPRE` AS pedidovendas_NRPEDREPRE,
     participante.`CODIGO` AS participante_CODIGO,
     tabelapreco.`CODIGO` AS tabelapreco_CODIGO,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     participante.`CNPJCPF` AS participante_CNPJCPF,
   participante.`EMAIL` AS participante_EMAIL,
     item.`CODIGO` AS item_CODIGO,
     enderecoparticipante.`COMPLEMENTO` AS enderecoparticipante_COMPLEMENTO,
     enderecoparticipante.`LOGRADOURO` AS enderecoparticipante_LOGRADOURO,
     enderecoparticipante.`CIDADE` AS enderecoparticipante_CIDADE,
     enderecoparticipante.`UF` AS enderecoparticipante_UF,
     enderecoparticipante.`COD_CEP` AS enderecoparticipante_CEP,
     enderecoparticipante.`NUMERO` AS enderecoparticipante_NRO,
     subcatitem.`DESCRICAO` AS subcatitem_DESCRICAO,
     enderecoparticipante.`BAIRRO` AS enderecoparticipante_BAIRRO,
     usuario.`APELIDO` AS usuario_APELIDO,
     participante.`FONE` AS participante_FONE,
     item.`DESCRICAO` AS item_DESCRICAO,
     case WHEN itempedidovendas.`QUANTIDADE` IS NOT null THEN itempedidovendas.`QUANTIDADE` WHEN itempedidovendas.`QUANTIDADE` is null THEN itempedidovendas.`QTDBONIFICADA` END as itempedidovendas_QUANTIDADE,
     itempedidovendas.`IPI` AS itempedidovendas_IPI,
     itempedidovendas.`VLUNIT` AS itempedidovendas_VLUNIT,
     itempedidovendas.`VLTOTLIQITEM` AS itempedidovendas_VLTOTLIQITEM,
     itempedidovendas.`VLTOTITEM` AS itempedidovendas_VLTOTITEM,
     itempedidovendas.`DESCONTO` AS itempedidovendas_DESCONTO,
     pedidovendas.`VLTOTLIQPEDIDO` AS pedidovendas_VLTOTLIQPEDIDO,
     pedidovendas.`OBSLOGISTICA` AS pedidovendas_OBSLOGISTICA,
     pedidovendas.`OBSPEDIDO` AS pedidovendas_OBSPEDIDO,
     enderecoparticipante.ie AS enderecoparticipante_ie,
     condpagamento.descricao as condpagamento_Descricao
FROM
     `pedidovendas` pedidovendas,
     `usuario` usuario,
     `itempedidovendas` itempedidovendas,
     `item` item,
     `tabelapreco` tabelapreco,
     `participante` participante,
     `enderecoparticipante` enderecoparticipante,
     `subcatitem` subcatitem,
     `condpagamento` condpagamento
WHERE
     pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     and pedidovendas.`CONDPAGAMENTO_id` = condpagamento.`id`
     and pedidovendas.`TABELAPRECO_id` = tabelapreco.`id`
     and pedidovendas.`CLIENTE_id` = participante.`id`
     and participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     and itempedidovendas.`ITEM_id` = item.`id`
     and item.`SUBCATITEM_id` = subcatitem.`id`
     and usuario.`id` = pedidovendas.`USUARIOREP_id`
     and itempedidovendas.`QTDBONIFICADA` IS NOT null
     and pedidovendas.`EMPRESA_id` = $P{idEmpresa}
     and pedidovendas.`NRPEDIDO` >= $P{iCodIni}
     and pedidovendas.`NRPEDIDO` <= $P{iCodFin}
ORDER BY
        pedidovendas.`NRPEDIDO`, item.`DESCRICAO` ASC]]>
	</queryString>

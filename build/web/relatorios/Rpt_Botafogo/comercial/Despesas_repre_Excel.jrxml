	<queryString>
		<![CDATA[SELECT
     despesasrep.`DATAMOVTO` AS despesasrep_DATAMOVTO,
     CASE WEEKDAY(despesasrep.`DATAMOVTO`) 
            WHEN 0 THEN 'Segunda-feira'
            WHEN 1 THEN 'Terça-feira'
            WHEN 2 THEN 'Quarta-feira'
            WHEN 3 THEN 'Quinta-feira' 
            WHEN 4 THEN 'Sexta-feira'
            WHEN 5 THEN 'Sabado'
            WHEN 6 THEN 'Domingo'
     END DIASEMANA, 
     despesasrep.`TIPO` AS despesasrep_TIPO,
     despesasrep.`VALOR` AS despesasrep_VALOR,
     despesasrep.`OBS` AS despesasrep_OBS,
     CASE 
        when (despesasrep.`TIPO` = 'Combustível KM rodado') THEN despesasrep.`QTD`
        ELSE null
     END despesasrep_QTD,
     representante.`APELIDO` AS representante_APELIDO,
     representante.`NOME` AS representante_NOME,
     empresa.`logoimg` AS empresa_logo,
     representante.`CODIGO` AS representante_CODIGO
FROM
     `representante` representante INNER JOIN `despesasrep` despesasrep ON representante.`id` = despesasrep.`REPRESENTANTE_id`
     INNER JOIN `empresa` empresa ON despesasrep.`EMPRESA_id` = empresa.`id`
$P!{cCondicao}
ORDER BY representante_NOME, despesasrep_TIPO,despesasrep_DATAMOVTO]]>
	</queryString>

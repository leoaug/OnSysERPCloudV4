	<queryString>
		<![CDATA[SELECT pedidovendas.`DESCPROD` AS pedidovendas_DESCPROD,
     pedidovendas.`DATAEMISSAO` AS pedidovendas_DATAEMISSAO,
     pedidovendas.`DATAENTREGA` AS pedidovendas_DATAENTREGA,
     tipodepedido.`DESCRICAO`   AS pedidovendas_TPPEDIDO,
     pedidovendas.`NRPEDIDO` AS pedidovendas_NRPEDIDO,
pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     pedidovendas.`PERCFRETE` AS pedidovendas_PERCFRETE,
     pedidovendas.`NRPEDREPRE` AS pedidovendas_NRPEDREPRE,
     pedidovendas.`ENTREGAPROGRAMADA` AS pedidovendas_ENTREGAPROGRAMADA,
     pedidovendas.`ENTREGAAGENDADA` AS pedidovendas_ENTREGAAGENDADA,
     case when pedidovendas.`TIPOPEDIDO` = 'BO' THEN "Ped. Original" ELSE
          (case when pedidovendas.`ENTREGAPROGRAMADA` THEN "ENT. PROG.: " ELSE "" END) END PROGRAMADA,
     case when pedidovendas.`TIPOPEDIDO` = 'BO' THEN (SELECT PO.`NRPEDCLI` FROM `pedidovendas` PX INNER JOIN `pedidovendas` PO ON PX.`PEDIDOORIGINAL_ID` = PO.id
            WHERE PX.`EMPRESA_id` = pedidovendas.`EMPRESA_id` AND PX.`NRPEDCLI` = pedidovendas.`NRPEDCLI`)
         ELSE
          (case when pedidovendas.`ENTREGAPROGRAMADA` AND pedidovendas.`MESENTREGAPROGRAMADA` IS NOT NULL THEN CONCAT("mês: ", pedidovendas.`MESENTREGAPROGRAMADA`, " - ano: ", pedidovendas.`ANOENTREGAPROGRAMADA`)
     when pedidovendas.`ENTREGAPROGRAMADA` AND pedidovendas.`DTENTREGAPROGRAMADA`  IS NOT NULL THEN CONCAT(pedidovendas.`DTENTREGAPROGRAMADA`, " - dias")
 ELSE "" END) END PROGRAMADA_DET,
     case when pedidovendas.`ENTREGAAGENDADA` THEN "ENT. AGEND.: "
     ELSE "" END AGENDADA,
     pedidovendas.`CONTATOENTREGAAGENDADA` AS pedidovendas_CONTATOENTREGAAGENDADA,
     pedidovendas.`CAMPANHA_ID` AS pedidovendas_CAMPANHA_id,
     case when pedidovendas.`CAMPANHA_ID` IS NOT NULL THEN (SELECT C.`DESCRICAO` FROM CAMPANHA C WHERE C.ID = pedidovendas.`CAMPANHA_ID`)
     ELSE "" END CAMPANHA_COD,
     participante.`CODIGO` AS participante_CODIGO,
     tabelapreco.`CODIGO` AS tabelapreco_CODIGO,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     participante.`CNPJCPF` AS participante_CNPJCPF,
   participante.`EMAIL` AS participante_EMAIL,
     item.`CODIGO` AS item_CODIGO,
     enderecoparticipante.`COMPLEMENTO` AS enderecoparticipante_COMPLEMENTO,
     enderecoparticipante.`LOGRADOURO` AS enderecoparticipante_LOGRADOURO,
     enderecoparticipante.`CIDADE` AS enderecoparticipante_CIDADE,
     enderecoparticipante.`UF` AS enderecoparticipante_UF,
     enderecoparticipante.`COD_CEP` AS enderecoparticipante_CEP,
     enderecoparticipante.`NUMERO` AS enderecoparticipante_NRO,
     subcatitem.`DESCRICAO` AS subcatitem_DESCRICAO,
     enderecoparticipante.`BAIRRO` AS enderecoparticipante_BAIRRO,
     representante.`APELIDO` AS representante_APELIDO,
empresa.`logoimg`  AS empresa_logo,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`FONE` AS representante_FONE,
     representante.`EMAIL` AS representante_EMAIL,
     participante.`FONE` AS participante_FONE,
     item.`DESCRICAO` AS item_DESCRICAO,
     case WHEN itempedidovendas.`QUANTIDADE` IS NOT null THEN itempedidovendas.`QUANTIDADE` WHEN itempedidovendas.`QUANTIDADE` is null THEN itempedidovendas.`QTDBONIFICADA` END as itempedidovendas_QUANTIDADE,
     itempedidovendas.`IPI` AS itempedidovendas_IPI,
 itempedidovendas.`OBS` AS itempedidovendas_OBS,
     itempedidovendas.`DESCONTO` AS itempedidovendas_DESCONTO,
     itempedidovendas.`VLICMSSUBSTITU` AS itempedidovendas_VLICMSSUBSTITU,
     itempedidovendas.`VLUNIT` AS itempedidovendas_VLUNIT,
     itempedidovendas.`VLUNITLIQ` AS itempedidovendas_VLUNITLIQ,
     itempedidovendas.`VLTOTLIQITEM` AS itempedidovendas_VLTOTLIQITEM,
     itempedidovendas.`VLTOTITEM` AS itempedidovendas_VLTOTITEM,
     itempedidovendas.`VLTOT` AS itempedidovendas_VLTOT,
     itempedidovendas.`PERCDESC` AS itempedidovendas_PERCDESC,
     itempedidovendas.`PERCICMS` AS itempedidovendas_PERCICMS,
     itempedidovendas.`PERCFRETE` AS itempedidovendas_PERCFRETE,
     itempedidovendas.`PERCIPI` AS itempedidovendas_PERCIPI,
     itempedidovendas.`PERCST` AS itempedidovendas_PERCST,
     pedidovendas.`VLTOTLIQPEDIDO` AS pedidovendas_VLTOTLIQPEDIDO,
     pedidovendas.`VLTOTPEDIDO` AS pedidovendas_VLTOTPEDIDO,
     pedidovendas.`OBSLOGISTICA` AS pedidovendas_OBSLOGISTICA,
     pedidovendas.`OBSPEDIDO` AS pedidovendas_OBSPEDIDO,
     enderecoparticipante.ie AS enderecoparticipante_ie,
     condpagamento.descricao as condpagamento_Descricao,
     enderecoEntraga.LOGRADOURO AS enderecoEntraga_LOGRADOURO,
     enderecoEntraga.CIDADE AS enderecoEntraga_CIDADE,
     enderecoEntraga.UF AS enderecoEntraga_UF,
     enderecoEntraga.BAIRRO AS enderecoEntraga_BAIRRO,
     enderecoEntraga.COD_CEP AS enderecoEntraga_COD_CEP,
     transportadora.`NOMEFANTASIA` AS transportadora_NOMEFANTASIA,
     transredesp.`NOMEFANTASIA` AS transredesp_NOMEFANTASIA
FROM
     `pedidovendas` pedidovendas INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
     INNER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     INNER JOIN `subcatitem` subcatitem ON item.`SUBCATITEM_id` = subcatitem.`id`
     INNER JOIN `tipodepedido` tipodepedido ON pedidovendas.`TIPODEPEDIDO_ID` = tipodepedido.`id`
     INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     INNER JOIN `empresa`       empresa            ON participante.`empresa_id`       = empresa.`id`
     INNER JOIN `representante` representante      ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     LEFT OUTER JOIN `regiao` regiao               ON representante.`REGIAO_id`       = regiao.`id`
     LEFT OUTER JOIN `tabelapreco` tabelapreco     ON pedidovendas.`TABELAPRECO_id`   = tabelapreco.`id`
     LEFT OUTER JOIN `condpagamento` condpagamento ON pedidovendas.`CONDPAGAMENTO_id` = condpagamento.`id`
     LEFT OUTER JOIN `enderecoparticipante` enderecoEntraga ON pedidovendas.`ENDERECOENTREGA_ID` = enderecoEntraga.`id`
     LEFT OUTER JOIN `transportadora` transportadora ON pedidovendas.`TRANSPORTADORA_id` = transportadora.`id`
     LEFT OUTER JOIN `transportadora` transredesp ON pedidovendas.`TRANSPREDES_id` = transredesp.`id`
WHERE itempedidovendas.`QUANTIDADE` IS NOT null
     and pedidovendas.`EMPRESA_id` = $P{idEmpresa}
     and pedidovendas.`ID` >= $P{iCodIni}
     and pedidovendas.`ID` <= $P{iCodFin}
ORDER BY
        pedidovendas.`NRPEDIDO`, item.`DESCRICAO` ASC]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     CASE WHEN (usuario.`TIPO` = 'Vendedor') THEN
        (CASE WHEN ((SELECT SUM(1)
                       FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
                               INNER JOIN `representante`    representanteX   ON representanteX.`id` = pedidovendasX.`REPRESENTANTE_id`
                               INNER JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`representante_id` = representanteX.`id` and
                                          metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
                       WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa}) AND (pedidovendasX.`DATAEMISSAO` >= $P{pDataIni} AND pedidovendasX.`DATAEMISSAO` <= $P{pDataFim}) AND
                               (representanteX.`id` = representante.`id` and pedidovendasX.VLTOTPEDIDO > 0 and pedidovendasX.TIPOPEDIDO <> 'BO'
         AND pedidovendasX.SITUACAO <> 'Cancelado'
         AND pedidovendasX.SITUACAO <> 'Pendente'
         AND pedidovendasX.SITUACAO <> 'Bloqueado'
         and pedidovendasX.SITUACAO <> 'Obs')) >= metapremiorepres.`METAPEDIDO`) and metapremiorepres.`METAPEDIDO` <> 0 THEN (300.00 / (SELECT SUM(1)
                       FROM `pedidovendas` pedidovendask INNER JOIN `participante` participantek ON pedidovendask.`CLIENTE_id` = participantek.`id`
          INNER JOIN `representante` representantek ON participantek.`REPRESENTANTE_id` = representantek.`id`
     LEFT JOIN `metapremiorepres` metapremiorepresk ON metapremiorepresk.`representante_id` = representantek.`id` and
                                   metapremiorepresk.ano = year($P{pDataIni}) and metapremiorepresk.mes = month($P{pDataIni})
     INNER JOIN `empresa` empresak ON empresak.`id` = pedidovendask.`EMPRESA_id`
     LEFT OUTER JOIN `usuario` usuariok ON representantek.`USUARIOREP_id` = usuariok.id
where pedidovendask.`EMPRESA_ID` = $P{cEmpresa} AND (pedidovendask.VLTOTPEDIDO > 0) AND pedidovendask.TIPOPEDIDO <> 'BO' AND  (pedidovendask.SITUACAO <> 'Cancelado') AND (pedidovendask.SITUACAO <> 'Bloqueado') AND (pedidovendask.SITUACAO <> 'Obs') AND (pedidovendask.SITUACAO <> 'Pendente') AND (pedidovendask.`DATAEMISSAO` BETWEEN $P{pDataIni} AND $P{pDataFim})  AND (pedidovendask.representante_id = representante.`id`)))
            ELSE
               0 END)
    ELSE
        (CASE WHEN ((SELECT SUM(1)
                        FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
                                INNER JOIN `representante`    representanteX   ON representanteX.`id` = pedidovendasX.`REPRESENTANTE_id`
                                INNER JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`representante_id` = representanteX.`id` and
                                           metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
                        WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa}) AND (pedidovendasX.`DATAEMISSAO` >= $P{pDataIni} AND pedidovendasX.`DATAEMISSAO` <= $P{pDataFim}) AND
                                (representanteX.`id` = representante.`id` and pedidovendasX.VLTOTPEDIDO > 0 AND pedidovendasX.TIPOPEDIDO <> 'BO'
          AND pedidovendasX.SITUACAO <> 'Cancelado'
          AND pedidovendasX.SITUACAO <> 'Pendente'
          AND pedidovendasX.SITUACAO <> 'Bloqueado'
          and pedidovendasX.SITUACAO <> 'Obs')) >= metapremiorepres.`METAPEDIDO`) and metapremiorepres.`METAPEDIDO` <> 0 THEN (pedidovendas.`VLTOTPEDIDO` * (pedidovendas.`PERCCOMISSAO` / 100)) * (4 / 100)
             ELSE
                0 END) END AS PremQtdPed,

     CASE WHEN (usuario.`TIPO` = 'Vendedor') THEN
          (CASE WHEN ((SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
		FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
			INNER JOIN `representante` representanteX ON representanteX.`id` = pedidovendasX.`REPRESENTANTE_id`
                        INNER JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`representante_id` = representanteX.`id` and
                                   metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
		WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa}) AND (pedidovendasX.`DATAEMISSAO` >= $P{pDataIni} AND pedidovendasX.`DATAEMISSAO` <= $P{pDataFim}) AND
			(representanteX.`id` = representante.`id` and pedidovendasX.VLTOTPEDIDO > 0 AND pedidovendasX.TIPOPEDIDO <> 'BO'
           AND pedidovendasX.SITUACAO <> 'Cancelado'
           AND pedidovendasX.SITUACAO <> 'Bloqueado'
           AND pedidovendasX.SITUACAO <> 'Pendente'
           and pedidovendasX.SITUACAO <> 'Obs')) >= metapremiorepres.`METAVALOR`) and metapremiorepres.`METAVALOR` <> 0  THEN (300.00 / (SELECT SUM(1)
                       FROM `pedidovendas` pedidovendask INNER JOIN `participante` participantek ON pedidovendask.`CLIENTE_id` = participantek.`id`
     INNER JOIN `representante` representantek ON participantek.`REPRESENTANTE_id` = representantek.`id`
     LEFT JOIN `metapremiorepres` metapremiorepresk ON metapremiorepresk.`representante_id` = representantek.`id` and
                                   metapremiorepresk.ano = year($P{pDataIni}) and metapremiorepresk.mes = month($P{pDataIni})
     INNER JOIN `empresa` empresak ON empresak.`id` = pedidovendask.`EMPRESA_id`
     LEFT OUTER JOIN `usuario` usuariok ON representantek.`USUARIOREP_id` = usuariok.id
where pedidovendask.`EMPRESA_ID` = $P{cEmpresa} AND (pedidovendask.VLTOTPEDIDO > 0) AND pedidovendask.TIPOPEDIDO <> 'BO' AND (pedidovendask.SITUACAO <> 'Cancelado') AND (pedidovendask.SITUACAO <> 'Bloqueado') AND (pedidovendask.SITUACAO <> 'Obs') AND (pedidovendask.`DATAEMISSAO` BETWEEN $P{pDataIni} AND $P{pDataFim})  AND (pedidovendask.representante_id = representante.`id`)))
           ELSE
	       0 END)
      ELSE
           (CASE WHEN ((SELECT SUM(pedidovendasX.`VLTOTPEDIDO`)
		FROM `participante` participanteX INNER JOIN `pedidovendas` pedidovendasX ON participanteX.`id` = pedidovendasX.`CLIENTE_id`
			INNER JOIN `representante` representanteX ON representanteX.`id` = pedidovendasX.`REPRESENTANTE_id`
                        INNER JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`representante_id` = representanteX.`id` and
                                   metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
		WHERE (pedidovendasX.`EMPRESA_id` = $P{cEmpresa}) AND (pedidovendasX.`DATAEMISSAO` >= $P{pDataIni} AND pedidovendasX.`DATAEMISSAO` <= $P{pDataFim}) AND
			(representanteX.`id` = representante.`id` and pedidovendasX.VLTOTPEDIDO > 0
             AND pedidovendasX.SITUACAO <> 'Cancelado'
             AND pedidovendasX.SITUACAO <> 'Bloqueado'
             AND pedidovendasX.SITUACAO <> 'Pendente'
             and pedidovendasX.SITUACAO <> 'Obs')) >= metapremiorepres.`METAVALOR`) and metapremiorepres.`METAVALOR` <> 0  THEN (pedidovendas.`VLTOTPEDIDO` * (pedidovendas.`PERCCOMISSAO` / 100)) * (6 / 100)
             ELSE
	        0 END) END AS  PremValorPed,

CASE WHEN ((SELECT MIN(pedidovendasCN.`NRPEDCLI`) FROM `participante` as  participanteCN, `pedidovendas` as  pedidovendasCN
                     where   participanteCN.`id`     = pedidovendasCN.`CLIENTE_id`
                       and   participanteCN.`codigo` = participante.`CODIGO`
		       and   pedidovendasCN.`EMPRESA_id` = $P{cEmpresa} AND pedidovendasCN.`DATAEMISSAO` <= $P{pDataFim}
                       AND  (pedidovendasCN.VLTOTPEDIDO > 0
  AND pedidovendasCN.SITUACAO <> 'Cancelado'
  AND pedidovendasCN.SITUACAO <> 'Pendente'
  AND pedidovendasCN.SITUACAO <> 'Bloqueado'
  and pedidovendasCN.SITUACAO <> 'Obs') AND (participante.`DATACADASTRO` >= $P{pDataIni})) = pedidovendas.`NRPEDCLI`) THEN pedidovendas.`VLTOTPEDIDO` * (pedidovendas.`PERCCOMISSAO` / 100)
  ELSE 0 END AS PremNovoCli,
     pedidovendas.`NRPEDCLI` AS doctosai_NRODOCTO,
     '' AS serienotafiscal_SERIE,
     metapremiorepres.`METAPEDIDO`,
     metapremiorepres.`METAVALOR`,
     representante.`CODIGO` AS representante_CODIGO,
     representante.`NOME` AS representante_NOME,
     participante.`CODIGO` AS participante_CODIGO,
     pedidovendas.`DATAEMISSAO` AS doctosai_DTEMISSAO,
     empresa.`logoimg` AS empresa_logo,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     pedidovendas.`VLTOTPEDIDO`  AS pedidovendas_VLTOTLIQPEDIDO,
     pedidovendas.`PERCCOMISSAO` AS pedidovendas_PERCCOMISSAO,
     pedidovendas.`VLTOTPEDIDO` * (pedidovendas.`PERCCOMISSAO` / 100) AS pedidovendas_VLCOMISSAO,
     pedidovendas.`NRPEDCLI` AS pedidovendas_NRPEDCLI,
     pedidovendas.`PERCDESCTOTAL` AS pedidovendas_PERCDESCTOTAL,
     0 AS IR
FROM
     `pedidovendas` pedidovendas INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `representante` representante ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     LEFT JOIN `metapremiorepres` metapremiorepres ON metapremiorepres.`representante_id` = representante.`id` and
                                   metapremiorepres.ano = year($P{pDataIni}) and metapremiorepres.mes = month($P{pDataIni})
     INNER JOIN `empresa` empresa ON empresa.`id` = pedidovendas.`EMPRESA_id`
     LEFT OUTER JOIN `usuario` usuario ON representante.`USUARIOREP_id` = usuario.id
$P!{cCondicao}
 ORDER BY representante.`CODIGO`, pedidovendas.`DATAEMISSAO`, pedidovendas.`NRPEDCLI`]]>
	</queryString>

	<queryString>
		<![CDATA[SELECT
     cep.`BAIRRO` AS cep_BAIRRO,
     cep.`CEP` AS cep_CEP,
     cep.`LOGRADOURO` AS cep_LOGRADOURO,
     municipio.`DESCRICAO` AS municipio_DESCRICAO,
     estado.`UF` AS estado_UF
FROM
     `estado` estado INNER JOIN `cep` cep ON estado.`id` = cep.`ESTADO_id`
     INNER JOIN `municipio` municipio ON estado.`id` = municipio.`ESTADO_id`
     AND municipio.`id` = cep.`MUNICIPIO_id`
WHERE
     cep.`CEP` >= $P{cCodIni}
     and cep.`CEP` <= $P{cCodFin}
order by estado.`UF`, municipio.`DESCRICAO`,  cep.`CEP`]]>

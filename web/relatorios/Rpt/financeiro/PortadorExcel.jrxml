<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="PortadorExcel" language="groovy" pageWidth="595" pageHeight="15000" columnWidth="595" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="zebra" isDefault="false" forecolor="#FFFFFF">
		<conditionalStyle>
			<conditionExpression><![CDATA[new Boolean($V{REPORT_COUNT}.intValue() % 2 == 0)]]></conditionExpression>
			<style isDefault="false" style="zebra" mode="Opaque" forecolor="#FFFFFF" backcolor="#CCFFFF"/>
		</conditionalStyle>
	</style>
	<parameter name="idEmpresa" class="java.lang.Integer"/>
	<parameter name="cCodIni" class="java.lang.String"/>
	<parameter name="cCodFin" class="java.lang.String"/>
	<parameter name="logo" class="java.awt.Image"/>
	<parameter name="cPar" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     portador.`CODIGO` AS portador_CODIGO,
     portador.`DESCRICAO` AS portador_DESCRICAO,
     portador.`AGENCIA` AS portador_AGENCIA,
     portador.`CONTA` AS portador_CONTA,
     bcofebraban.`DESCRICAO` AS bcofebraban_DESCRICAO,
     bcofebraban.`CODIGO` AS bcofebraban_CODIGO,
     portador.`SALDO` AS portador_SALDO
FROM
     `bcofebraban` bcofebraban RIGHT OUTER JOIN `portador` portador ON bcofebraban.`id` = portador.`BANCO_id`
WHERE
     portador.`EMPRESA_id` = $P{idEmpresa}
     and portador.`CODIGO` >= $P{cCodIni}
     and portador.`CODIGO` <= $P{cCodFin}]]>
	</queryString>
	<field name="portador_CODIGO" class="java.lang.String"/>
	<field name="portador_DESCRICAO" class="java.lang.String"/>
	<field name="portador_AGENCIA" class="java.lang.String"/>
	<field name="portador_CONTA" class="java.lang.String"/>
	<field name="bcofebraban_DESCRICAO" class="java.lang.String"/>
	<field name="bcofebraban_CODIGO" class="java.lang.String"/>
	<field name="portador_SALDO" class="java.math.BigDecimal"/>

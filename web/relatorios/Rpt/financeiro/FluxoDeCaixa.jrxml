	<queryString>
		<![CDATA[select "CONTAS A RECEBER(+)" AS Conta,
vencimentogerencial AS PERIODO,
portador.`DESCRICAO`,
SUBSTRING(VENCIMENTOGERENCIAL, 7, 4) as ano,
SUBSTRING(VENCIMENTOGERENCIAL, 4, 2) as mes,
SUBSTRING(VENCIMENTOGERENCIAL, 1, 2) as dia,
SUM(tituloarparcela.`VALORPARCELA`) AS PREVISTO,
SUM(tituloarparcela.`VALORPARCELA` - tituloarparcela.`VLSALDO`) AS REALIZADO,
SUM(tituloarparcela.`VLSALDO`) AS SALDO,
SUM(tituloarparcela.`VALORPARCELA` - (tituloarparcela.`VALORPARCELA` - tituloarparcela.`VLSALDO`)) AS DIFERENCA
from tituloar INNER JOIN `empresa` empresa ON tituloar.`EMPRESA_id` = empresa.id INNER JOIN
`portador` portador ON tituloar.`PORTADOR_id` = portador.id INNER JOIN
`tituloarparcela` tituloarparcela ON tituloar.id = tituloarparcela.`TITULOAR_id`
WHERE tituloarparcela.`DATAVENCIMENTO`
$P!{cCondicao}
GROUP BY Conta, portador.`DESCRICAO`, SUBSTRING(VENCIMENTOGERENCIAL, 7, 4), SUBSTRING(VENCIMENTOGERENCIAL, 4, 2), SUBSTRING(VENCIMENTOGERENCIAL, 1, 2), vencimentogerencial
UNION
select "CONTAS A PAGAR(-)" AS Conta,
vencimentogerencial AS PERIODO,
portador.`DESCRICAO`,
SUBSTRING(VENCIMENTOGERENCIAL, 7, 4) as ano,
SUBSTRING(VENCIMENTOGERENCIAL, 4, 2) as mes,
SUBSTRING(VENCIMENTOGERENCIAL, 1, 2) as dia,
(SUM(tituloapparcela.`VALORPARCELA`)) * -1 AS PREVISTO,
(SUM(tituloapparcela.`VALORPARCELA` - tituloapparcela.`VLSALDO`)) * -1 AS REALIZADO,
(SUM(tituloapparcela.`VLSALDO`)) * -1 AS SALDO,
(SUM(tituloapparcela.`VALORPARCELA` - (tituloapparcela.`VALORPARCELA` - tituloapparcela.`VLSALDO`))) * -1 AS DIFERENCA
from tituloap INNER JOIN `empresa` empresa ON tituloap.`EMPRESA_id` = empresa.id INNER JOIN
`portador` portador ON tituloap.`PORTADOR_id` = portador.id INNER JOIN
`tituloapparcela` tituloapparcela ON tituloap.id = tituloapparcela.`TITULOAP_id`
WHERE tituloapparcela.`DATAVENCIMENTO`
$P!{cCondicao2}
GROUP BY Conta, portador.`DESCRICAO`, SUBSTRING(VENCIMENTOGERENCIAL, 7, 4), SUBSTRING(VENCIMENTOGERENCIAL, 4, 2), SUBSTRING(VENCIMENTOGERENCIAL, 1, 2), vencimentogerencial
ORDER BY Conta, ano, mes, dia]]>

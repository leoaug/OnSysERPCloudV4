     item.`CODIGO` AS item_CODIGO,
     tabelapreco.`EMPRESA_id` AS tabelapreco_EMPRESA_id
FROM
     `tabelapreco` tabelapreco INNER JOIN `itemtabelapreco` itemtabelapreco ON tabelapreco.`id` = itemtabelapreco.`TABELAPRECO_id`
     INNER JOIN `item` item ON itemtabelapreco.`ITEM_id` = item.`id`

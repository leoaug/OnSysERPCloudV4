	<field name="pedidovendas_OBSCREDITO" class="java.lang.String"/>
	<field name="pedidovendas_obsintegra" class="java.lang.String"/>
	<variable name="Observacao" class="java.lang.String">
		<variableExpression><![CDATA[(!$F{pedidovendas_OBSLOGISTICA}.isEmpty() ? $F{pedidovendas_OBSLOGISTICA} : ( !$F{pedidovendas_OBSCREDITO}.isEmpty() ? $F{pedidovendas_OBSCREDITO} :
    ( !$F{pedidovendas_OBSTROCACONDPAG}.isEmpty() ? $F{pedidovendas_OBSTROCACONDPAG} :
        ( !$F{pedidovendas_OBSLOGISTICA}.isEmpty() ? $F{pedidovendas_OBSLOGISTICA} :
            ( !$F{pedidovendas_OBSTROCATABELA}.isEmpty() ? $F{pedidovendas_OBSTROCATABELA} :
                ( !$F{pedidovendas_obsintegra}.isEmpty() ? $F{pedidovendas_obsintegra}: $F{pedidovendas_OBSPEDIDO} ) ) ) ) ) )]]></variableExpression>
	</variable>

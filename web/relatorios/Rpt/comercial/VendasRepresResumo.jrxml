	<queryString>
		<![CDATA[SELECT
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     CASE pedidovendas.`SITUACAO`
                WHEN 'Faturado' THEN 'Realizado'
                ELSE 'Carteira' END TipoSit,
     SUM(itempedidovendas.`QUANTIDADE`) AS Caixa,
     SUM(itempedidovendas.`VLTOTITEM`) AS VlTab,
     SUM(itempedidovendas.`VLTOTLIQITEM`) AS VlMercad
FROM
     `regiao` regiao INNER JOIN `representante` representante ON regiao.`id` = representante.`REGIAO_id`
     INNER JOIN `participante` participante ON representante.`id` = participante.`REPRESENTANTE_id`
     INNER JOIN `pedidovendas` pedidovendas ON participante.`id` = pedidovendas.`CLIENTE_id`
     INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id`
WHERE (representante.`EMPRESA_ID` = $P{idEmpresa}) AND (pedidovendas.`DATAEMISSAO` BETWEEN $P{dtIni} AND $P{dtFim})
GROUP BY regiao.`DESCRICAO`, TipoSit
ORDER BY regiao_DESCRICAO]]>
	</queryString>

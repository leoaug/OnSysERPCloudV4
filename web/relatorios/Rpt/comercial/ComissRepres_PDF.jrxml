	<queryString>
		<![CDATA[SELECT movtofin.`ORIGEM` AS Especie,
       movtofin.`SERIE` AS Serie,
       movtofin.`NRODOCTO` AS Titulo,
       tituloarparcela.`PARCELA` AS Parcela,
       representante.`APELIDO` AS NomeAbrev,
       movtofin.`DTMOVTO` AS Emissao,
       tituloarparcela.`DATAVENCIMENTO` AS Vencimento,
       representante.`COMISSAO` AS Percentual,
       empresa.`logoimg` AS empresa_logo,
       movtofin.`VLMOVTO` AS Valor,
       doctosai.`NRPEDIDO` as NrPedido,
       CASE representante.`COMISSAO`
            when  representante.`COMISSAO` = '' THEN 0
            ELSE ((representante.`COMISSAO` * movtofin.`VLMOVTO`)/100) END ValComissao
FROM
     onsys.`movtofin` movtofin INNER JOIN onsys.`tituloarparcela` tituloarparcela ON movtofin.`tituloarparcela_id` = tituloarparcela.`id`
     INNER JOIN onsys.`doctosai` doctosai ON movtofin.`nrodocto` = doctosai.`nrodocto`
     INNER JOIN onsys.`participante` participante ON movtofin.`PARTICIPANTE_id` = participante.`id`
     INNER JOIN onsys.`representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `empresa` empresa ON empresa.`id` = movtofin.`EMPRESA_id`
$P!{cCondicao}
order by representante.`APELIDO`, movtofin.`DTMOVTO`]]>
	</queryString>

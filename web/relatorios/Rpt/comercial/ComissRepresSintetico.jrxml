	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<style name="zebra" isDefault="false" mode="Opaque" forecolor="#FFFFFF">
		<conditionalStyle>
			<conditionExpression><![CDATA[new Boolean($V{REPORT_COUNT}.intValue() % 2 == 0)]]></conditionExpression>
			<style isDefault="false" style="zebra" mode="Opaque" forecolor="#FFFFFF" backcolor="#CCFFFF"/>
		</conditionalStyle>
	</style>
	<parameter name="logo" class="java.awt.Image"/>
	<parameter name="cPar" class="java.lang.String"/>
	<parameter name="cCondicao" class="java.lang.String">
		<defaultValueExpression><![CDATA[" "]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT representante.`APELIDO` AS NomeAbrev,
       representante.`COMISSAO` AS Percentual,
       sum(movtofin.`VLMOVTO`) AS Valor,
       CASE representante.`COMISSAO`
            when  representante.`COMISSAO` = '' THEN 0
            ELSE ((representante.`COMISSAO` * SUM(movtofin.`VLMOVTO`))/100) END ValComissao
FROM
     onsys.`movtofin` movtofin INNER JOIN onsys.`tituloarparcela` tituloarparcela ON movtofin.`tituloarparcela_id` = tituloarparcela.`id`
     INNER JOIN onsys.`participante` participante ON movtofin.`PARTICIPANTE_id` = participante.`id`
     INNER JOIN onsys.`representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
$P!{cCondicao}
GROUP BY representante.`APELIDO`, representante.`COMISSAO`
order by representante.`APELIDO`]]>
	</queryString>
	<field name="NomeAbrev" class="java.lang.String"/>
	<field name="Percentual" class="java.math.BigDecimal"/>
	<field name="Valor" class="java.math.BigDecimal"/>
	<field name="ValComissao" class="java.math.BigDecimal"/>
	<variable name="totComissao" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{ValComissao}]]></variableExpression>
	</variable>

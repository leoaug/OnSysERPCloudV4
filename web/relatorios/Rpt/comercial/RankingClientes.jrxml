	<queryString>
		<![CDATA[SELECT participante.`CODIGO` AS Codigo,
       participante.`nomefantasia` AS NomeFantasia,
       sum(itemdoctosai.`VLTOTITEM`) AS Valor,

(SELECT sum(itemdoctosai.`VLTOTITEM`)
FROM
     `doctosai` doctosai INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
   $P!{cCondicao}
     order by sum(itemdoctosai.`VLTOTITEM`) desc limit 12 ) AS Valor_tot


FROM `doctosai` doctosai INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
     INNER JOIN `PARTICIPANTE` PARTICIPANTE ON PARTICIPANTE.`id` = doctosai.`participante_id`
$P!{cCondicao}
     group by participante.`CODIGO`
     order by sum(itemdoctosai.`VLTOTITEM`)   desc limit 12]]>
	</queryString>

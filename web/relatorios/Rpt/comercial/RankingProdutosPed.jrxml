	<queryString>
		<![CDATA[SELECT
     item.`CODIGO` AS Codigo,
     item.`DESCRICAO` AS Descricao,
     'valor_tot' as vvl_tot,
     empresa.`logoimg`  AS empresa_logo,
     sum(itempedidovendas.`VLTOT`) AS Valor,
     sum(itempedidovendas.`QUANTIDADE`) AS Quantidade,
    (sum(itempedidovendas.`VLTOT`)/sum(itempedidovendas.`QUANTIDADE`)) AS precoMedido,
(SELECT sum(pedidovendas.`VLTOTPEDIDO`)
FROM
    pedidovendas pedidovendas INNER JOIN `representante` representante ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     $P!{cCondicao}
     order by sum(pedidovendas.`VLTOTPEDIDO`) ) AS Valor_tot
FROM
    `pedidovendas` pedidovendas INNER JOIN `itempedidovendas` itempedidovendas ON pedidovendas.`id` = itempedidovendas.`PEDIDOVENDAS_id` 
     INNER JOIN `empresa`       empresa       ON pedidovendas.`empresa_id` = empresa.`id`
     INNER JOIN `item` item ON itempedidovendas.`ITEM_id` = item.`id`
     INNER JOIN `participante` participante ON pedidovendas.`CLIENTE_id` = participante.`id`
     INNER JOIN `representante` representante ON pedidovendas.`REPRESENTANTE_id` = representante.`id`
     INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
     LEFT OUTER JOIN `REGIAO` regiao ON representante.`REGIAO_id` = regiao.`id`
$P!{cCondicao}
group by item.`CODIGO`
order by sum(itempedidovendas.`VLTOT`) desc]]>
	</queryString>

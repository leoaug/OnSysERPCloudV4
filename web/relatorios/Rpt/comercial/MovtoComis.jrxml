     empresa.`logoimg`  AS empresa_logo,
     empresa.`DESCRICAO` AS empresa_DESCRICAO,
     comissaomovto.`DTMOVTO` AS comissaomovto_DTMOVTO,
     comissaomovto.`TPMOVTO` AS comissaomovto_TPMOVTO,
     comissaomovto.`VLMOVTO` AS comissaomovto_VLMOVTO,

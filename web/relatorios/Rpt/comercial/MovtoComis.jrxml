     comissaotpmov.`DESCRICAO` AS comissaotpmov_DESCRICAO,
     comissaotpmov.`TIPO` AS comissaotpmov_TIPO,
     representante.`APELIDO` AS representante_APELIDO,
     representante.`CODIGO` AS representante_CODIGO,
     comissaomovto.`HISTORICO` AS comissaomovto_HISTORICO
FROM
     `empresa` empresa INNER JOIN `comissaomovto` comissaomovto ON empresa.`id` = comissaomovto.`EMPRESA_id`
     INNER JOIN `comissaotpmov` comissaotpmov ON comissaomovto.`COMISSAOTPMOV_id` = comissaotpmov.`id`

	<queryString>
		<![CDATA[SELECT
     pedidovendas.`NRPEDIDO` AS nPedido,
     pedidovendas.`NRPEDCLI` AS pedWeb,
     pedidovendas.`SITUACAO` AS Situacao,
     participante.`CPF` AS participante_CPF,
     participante.`NOMEFANTASIA` AS Cliente,
     pedidovendas.`OBSPEDIDO` AS pedidovendas_OBSPEDIDO,
     pedidovendas.`OBSTROCACONDPAG` AS pedidovendas_OBSTROCACONDPAG,
     pedidovendas.`OBSTROCATABELA` AS pedidovendas_OBSTROCATABELA,
     pedidovendas.`OBSLOGISTICA` AS pedidovendas_OBSLOGISTICA,
     pedidovendas.`OBSCREDITO` AS pedidovendas_OBSCREDITO,
     pedidovendas.`obsintegra` AS pedidovendas_obsintegra,
     representante.`APELIDO` AS representante_APELIDO,
     representante.`CODIGO` AS representante_CODIGO,
     empresa.`logoimg` AS empresa_logo
FROM
     `participante` participante INNER JOIN `pedidovendas` pedidovendas ON participante.`id` = pedidovendas.`CLIENTE_id`
INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
INNER JOIN `empresa` empresa ON empresa.`id` = participante.`EMPRESA_id`
$P!{cCondicao}
order by representante_APELIDO]]>
	</queryString>

     ordprod.`PESOLIQ` AS ordprod_PESOLIQ,
     ordprod.`TIPOPROD` AS ordprod_TIPOPROD,
     ordprod.`NRPEDCLI` AS ordprod_NRPEDCLI,
     participante.`CODIGO` AS participante_CODIGO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     tt_onsys.`char1` AS tt_onsys_char1
FROM
     `participante` participante INNER JOIN `ordprod` ordprod ON participante.`id` = ordprod.`CLIENTE_id`
     INNER JOIN `item` item ON ordprod.`ITEM_id` = item.`id`,
     `tt-onsys` tt_onsys
$P!{cCondicao}
ORDER BY tt_onsys_char1]]>

	<queryString>
		<![CDATA[SELECT
     participante.`id` AS participante_id,
     participante.`CODIGO` AS participante_CODIGO,
     participante.`RAZAOSOCIAL` AS participante_RAZAOSOCIAL,
     participante.`TIPO` AS participante_TIPO,
     participante.`NOMEFANTASIA` AS participante_NOMEFANTASIA,
     participante.`CPF` AS participante_CPF,
     bcofebraban.`DESCRICAO` AS banco_DESCRICAO,
     bcofebraban.`CODIGO` AS banco_CODIGO,
     participante.`AGENCIA` AS participante_AGENCIA,
     participante.`DIGITOAGENCIA` AS participante_DIGITOAGENCIA,
     participante.`CONTACORRENTE` AS participante_CONTACORRENTE,
     participante.`DIGITOCONTA` AS participante_DIGITOCONTA,
     participante.`TIPOCONTA` AS participante_TIPOCONTA,
     participante.`CNPJCPF` AS participante_CNPJCPF,
     participante.`OBSPARTICIPANTE` AS participante_OBSPARTICIPANTE,
     CASE participante.`INATIVO`
	WHEN 'S' THEN "Inativos"
     ELSE
	"Ativos"
     END INATIVO,
     portador.`CODIGO` AS portador_CODIGO,
     enderecoparticipante.`BAIRRO` AS cep_BAIRRO,
     enderecoparticipante.`COD_CEP` AS cep_CEP,
     empresa.`logoimg`  AS empresa_logo,
     enderecoparticipante.`LOGRADOURO` AS cep_LOGRADOURO,
     enderecoparticipante.`COMPLEMENTO` AS enderecoparticipante_COMPLEMENTO,
     enderecoparticipante.`NUMERO` AS enderecoparticipante_NUMERO,
     enderecoparticipante.`CIDADE` AS estado_DESCRICAO,
     enderecoparticipante.`UF` AS estado_UF,
     representante.`NOME` AS representante_NOME,
     regiao.`DESCRICAO` AS regiao_DESCRICAO,
     comprador.`NOME` AS comprador_NOME
FROM
     `bcofebraban` bcofebraban RIGHT OUTER JOIN `participante` participante ON bcofebraban.`id` = participante.`BANCO_id`
    INNER JOIN `representante` representante ON participante.`REPRESENTANTE_id` = representante.`id`
    INNER JOIN `enderecoparticipante` enderecoparticipante ON participante.`id` = enderecoparticipante.`PARTICIPANTE_id`
    INNER JOIN `empresa`       empresa       ON participante.`empresa_id` = empresa.`id`
    LEFT OUTER JOIN `regiao` regiao ON representante.`REGIAO_id` = regiao.`id`
    LEFT OUTER JOIN `portador` portador ON participante.`PORTADOR_id` = portador.`id`
    LEFT OUTER JOIN `catfornecedor` catfornecedor ON participante.`CATFORNECEDOR_id` = catfornecedor.`id`
    LEFT OUTER JOIN `comprador` comprador ON participante.`COMPRADOR_id` = comprador.`id`
$P!{cCondicao}
ORDER BY regiao_DESCRICAO, representante_NOME, participante_RAZAOSOCIAL]]>

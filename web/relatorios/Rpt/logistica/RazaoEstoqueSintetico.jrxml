	<queryString>
		<![CDATA[select distinct I.`CODIGO`, I.`DESCRICAO`, deposito.`DESCRICAO` AS Deposito, loc.`DESCRICAO` AS Localizacao,
    (CASE
        WHEN (select sum(D.`QTDMOVTO`)
            FROM movtoestoque D
            $P!{cCondicaoS}) IS NULL THEN (select sum(B.`QTDMOVTO`)
            FROM movtoestoque B
            $P!{cCondicaoE})
         WHEN (select sum(B.`QTDMOVTO`)
            FROM movtoestoque B
            $P!{cCondicaoE}) IS NULL THEN (0 -
            (select sum(D.`QTDMOVTO`)
            FROM movtoestoque D
            $P!{cCondicaoS}))
         ELSE ((select sum(B.`QTDMOVTO`)
            FROM movtoestoque B
            $P!{cCondicaoE}) -
            (select sum(D.`QTDMOVTO`)
            FROM movtoestoque D
            $P!{cCondicaoS}))
     END) AS SaldoAnterior,
    (select sum(B.`QTDMOVTO`)
    FROM movtoestoque B
    $P!{cCondEntradas}) as Entrada,
    (select sum(D.`QTDMOVTO`)
    FROM movtoestoque D
    $P!{cCondSaidas}) as Saida,
    (CASE
        WHEN (select sum(D.`QTDMOVTO`)
            FROM movtoestoque D
            $P!{cCondicaoS2}) IS NULL THEN
                (select sum(B.`QTDMOVTO`)
            FROM movtoestoque B
            $P!{cCondicaoE2})
        ELSE
            ((select sum(B.`QTDMOVTO`)
            FROM movtoestoque B
            $P!{cCondicaoE2}) -
            (select sum(D.`QTDMOVTO`)
            FROM movtoestoque D
            $P!{cCondicaoS2}))
     END) AS SaldoAtual
from item I INNER JOIN saldoestoque C ON I.`id` = C.`ITEM_ID`
INNER JOIN deposito deposito ON C.`DEPOSITO_id` = deposito.`id`
     INNER JOIN localizacao loc ON C.`LOCALIZACAO_id` = loc.`id`
$P!{cCondicao}
ORDER BY I.`DESCRICAO`;]]>
	</queryString>

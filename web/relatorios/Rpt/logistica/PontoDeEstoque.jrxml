     saldoestoque.`QUANTIDADE` AS saldoestoque_QUANTIDADE,
     SUM(vsaldoestoquetemp.`SalAberto`) AS vsaldoestoquetemp_SalAberto,
     SUM(vsaldoestoquetemp.`SalLiberado`) AS vsaldoestoquetemp_SalLiberado
fROM
     `saldoestoque` saldoestoque INNER JOIN `vsaldoestoquetemp` vsaldoestoquetemp ON saldoestoque.`item_id` = vsaldoestoquetemp.`ID`
     INNER JOIN `localizacao` localizacao ON saldoestoque.`LOCALIZACAO_id` = localizacao.`id`
     INNER JOIN `deposito` deposito ON localizacao.`DEPOSITO_id` = deposito.`id`
$P!{cCondicao}
GROUP BY vsaldoestoquetemp.`CODIGO`, vsaldoestoquetemp.`DESCRICAO`, deposito.`DESCRICAO`, localizacao.`DESCRICAO`, saldoestoque.`QUANTIDADE`
ORDER BY vsaldoestoquetemp_DESCRICAO, deposito_DESCRICAO, localizacao_DESCRICAO]]>

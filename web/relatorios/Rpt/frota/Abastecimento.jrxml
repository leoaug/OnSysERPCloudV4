	<queryString>
		<![CDATA[SELECT v.`PLACA` AS Veiculo,
a.`KM`,
a.`OBS`,
a.`QUANTIDADE`,
a.`VALOR`,
a.`VEICULOS_id`,
a.`DATA`,
a.`KMINICIAL`,
a.`KMPERCORRIDO`,
a.`CONSUMO`,
e.`LOGOIMG`  AS empresa_logo,
v.`KMATUAL` AS inicialKM
FROM abastecimento a INNER JOIN empresa e ON a.`EMPRESA_id` = e.id
INNER JOIN veiculos v ON a.`VEICULOS_id` = v.id
$P!{cCondicao}
ORDER BY v.`PLACA`, a.`DATA`]]>
	</queryString>

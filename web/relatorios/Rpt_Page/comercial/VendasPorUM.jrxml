	<queryString>
		<![CDATA[SELECT
     item.`CODIGO` AS item_CODIGO,
     item.`DESCRICAO` AS item_DESCRICAO,
     unidademedida.`DESCRICAO` AS unidademedida_DESCRICAO,
     SUM(itemdoctosai.`QUANTIDADE`) AS itemdoctosai_QUANTIDADE,
     SUM(itemdoctosai.`VLTOTLIQITEM`) AS itemdoctosai_VLTOTLIQITEM
FROM
     `doctosai` doctosai INNER JOIN `itemdoctosai` itemdoctosai ON doctosai.`id` = itemdoctosai.`DOCTOSAI_id`
     INNER JOIN `item` item ON itemdoctosai.`ITEM_id` = item.`id`
     INNER JOIN `unidademedida` unidademedida ON item.`UNIDADEMEDIDA_id` = unidademedida.`id`
where  doctosai.`DTEMISSAO` BETWEEN '2014-04-01' AND '2014-04-30' and doctosai.empresa_id = 6
GROUP BY item.`CODIGO`, item.`DESCRICAO`, unidademedida.`DESCRICAO`
ORDER BY unidademedida_DESCRICAO, item_DESCRICAO ]]>
	</queryString>

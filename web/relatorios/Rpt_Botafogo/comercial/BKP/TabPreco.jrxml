     tabelapreco.`DATAVALINI` AS tabelapreco_DATAVALINI,
     tabelapreco.`DATAVALFIM` AS tabelapreco_DATAVALFIM,
     itemtabelapreco.`ITEM_id` AS itemtabelapreco_ITEM_id,
